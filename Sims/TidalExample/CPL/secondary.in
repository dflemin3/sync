# The host star, the sun -> Primary body!
sName		secondary		# Body's name
saModules       eqtide 

# Physical Parameters
iBodyType     1         # Star!
dMass         1.0       # Mass, solar masses
dRadius       0.00465   # 1 Rsun in AU
dRadGyra      0.27
dAge          5.0e6
dRotPeriod    -1.0

# Orbital properties
dOrbPeriod  -10.0
dEcc         0.2

# EQTIDE Parameters
dTidalQ		1e6		# Tidal phase lag
dK2		0.5		# Love number of degree 2
dMaxLockDiff    0.01
saTidePerts	primary   	# Body name(s) of tidal perturbers

saOutputOrder	Time -TotEn -TotAngMom -Semim -RotPer Ecce -OrbPer -SurfEnFluxTotal -OrbAngMom 
