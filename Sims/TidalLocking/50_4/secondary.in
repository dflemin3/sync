# The host star, the sun -> Primary body!
sName           secondary
saModules       stellar eqtide

# Physical Parameters
iBodyType       1           # This is a star!
dMass           1.0         # Mass, solar masses
dAge            1.0e6
dRotPeriod      -1.0

# Orbital properties
dOrbPeriod      -75.0
dEcc            0.2

# EQTIDE Parameters
dTidalQ         1.0e4       # Tidal phase lag
dK2             0.5         # Love number of degree 2
dMaxLockDiff    0.01
saTidePerts     primary     # Body name(s) of tidal perturbers

# Stellar param
sStellarModel   baraffe
sMagBrakingModel matt

saOutputOrder   Time -RotPer -OrbPer Ecce -TotEn -TotAngMom
